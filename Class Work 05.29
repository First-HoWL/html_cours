css



body{
	padding: 0;
	margin: 0;	
	font-family: "Roboto", sans-serif;
}

div.first_img{
	background-image: url(https://s3-alpha-sig.figma.com/img/6904/8776/b42c9c5d9ec5e4c972446e63e68a65a4?Expires=1717977600&Key-Pair-Id=APKAQ4GOSFWCVNEHN3O4&Signature=KfGBqXm7BkuLK85lQmzozSxLqZlaQ0jYYbKffDCTPCMfPtrxoOR~ITf2X4iGhpfBPlhRFGKz72BbWPO45Fb3VS31YYsBnHZAT-9idSM93X8sM44maUInGoc78~O4AlxX2iW8n9os8SSVoC9FBbyyE1LvdzSsBcUyH12Ew1KXsBAlQqlbt9uuOHEq34vYrIAhtupTbfbUzN9v4g2ioErss7uEwh1cNbzJ5NqHQHoNM2Ejnq23M61ynNdawOJnjzO0M0smp2PqaqAx5K7j6f-UoyupOV-YdyZH4nj8lC~p~tm70qUGXT5YrLP0BsF5uo6va-igJE7hGv9Tq5ub86CARQ__);
	height: 100vh;
	width: 100%;
	background-size: cover;
	background-repeat: no-repeat;
	background-position: center center;
}

.main{
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: flex-start;
}

nav{
	padding-top: 20px;
	padding-bottom: 20px;
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: flex-start;
}

.sect{
	color: grey;
	text-align: center;
	line-height: 40px;
	padding-right: 20px;
	padding-left: 20px;
	height: 40px;
	width: auto;
	transition: font-weight 0.9s, color 0.6s;
}

.sect:hover{
	color: white;
}

a{
	text-decoration: none;
}
.button{
	margin: 0;
	height: 40px;
	width: 120px;
	background-color: orange;
	border-radius: 10px;
	color: white;
	text-align: center;
	line-height: 40px;
	transition: font-weight 0.6s, transform 0.7s;
}

.button:hover{
	transform: scale(1.2);
}



section{
	color: white;
}

span.h5{
	font-size: 14px;
	font-weight: 500;
}

span.h4{
	font-size: 20px;
	font-weight: 500;
}

span.h2{
	font-size: 57.33px;
	font-weight: 500;
}

section > p{
	font-weight: 400;
	font-size: 18px;
	color: grey;
}

main.central_text{
	display: flex;
	flex-direction: column;
	justify-content: center;
	width: 49%;
}

nav > section.icon > div{
	height: 40px;
	overflow: hidden;
}

nav > section.icon > div > img{
	position: center center;
	height: 100px;
	width: 100px;
	margin-top: -27px;
	margin-left: -25px;
}

div.max_div{
	width: 70%;
	margin-left: auto;
	margin-right: auto; 
}

main > section:first-child{
	height: 250px;
}

div.second_box{
	background-color: #1C1C1C;
	height: 500px;
	display: flex;
	align-items: center;
	justify-content: center;
}

div.line{
	display: flex;
}

div.box{
	background-color: #312F30;
	text-align: center;
	color: white;
	border-radius: 10px;
	height: 323px;
	width: 262px;
	margin: 10px;
	box-sizing: border-box;
}

div.box > p{
	font-weight: 400;
	font-size: 16px;
	padding: 10px;
	color: #FFFFFF;
}

div.box > img{
	border-radius: 10px 10px 0 0;
	height: 175px;
	width: 262px;
}
