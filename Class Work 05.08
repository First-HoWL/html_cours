body{
    margin-left: 20%;
    margin-right: 20%;
    background-repeat: no-repeat;
    background-position: center center;
    background-attachment: fixed;
    background-size:cover;
    background-image: url(https://kartinki.pics/uploads/posts/2021-04/1617573903_25-p-polnostyu-chernii-fon-30.jpg)
}

#rick_roll{
    color: orange;
}

#max_div{
    background-color: rgba(255, 255, 255, 0.9);
    padding: 15px;
}

img{
    width: 60%;
}

.center{
    text-align: center;
}

#button{
    height: 50px;
    width: 50px;
    background-color: white;
    box-shadow: 2px;
    border-radius: 10px;
    font-size: 40px;
}

a{
    text-decoration: none;
}


h2{
    text-align: center;
    color: orange;
}


p{
color: orange;
}

.second{
    color: green;
}

.third{
    color: blue;
}

strong{
    color: black;
}

.ul_list{
    list-style-type: none;
    list-style-position: inside;
    padding-left: 10px;  
}

.top5{
    text-align: left;
}

ul.ul_list li::before{
    content: "";
    display: inline-block;
    width: 18px;
    height: 18px;
    background-image: url("https://cdn4.iconfinder.com/data/icons/basic-business-1/36/Checkmark_right_icon-512.png");
    background-size: contain;
    margin-right: 7px;
}
ul.ul_list li:nth-child(2n){
    color: #696969;
}

ul.ul_list li:nth-child(odd){
    color: coral;
}


