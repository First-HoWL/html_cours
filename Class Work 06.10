css



body{
    padding: 0;
    margin: 0;
    background-color: #000000;
    color: white;
}

nav{
    display: flex;
    justify-content: center;
    padding: 20px;
}

header{
    position: relative;
    background-color: #000000;
}
.buttons{
    display: flex;
    justify-content: center;
    text-transform: uppercase;
}

div.icon{
    background-image: url(Layer_1.png);
    height: 68px;
    width: 167px;
    display: inline-block;
}

a{
    display: inline-block;
    padding: 20px 40px;
    text-decoration: none;
    color: white;
}

div.icons{
    background-image: url(Vector.png);
    height: 40px;
    width: 40px;
    background-position: center center;
    background-size: 36px;
    background-repeat: no-repeat;
    margin-top: 8px;
}

div.icons_box{
    vertical-align: middle;
}

nav a:hover{
    background-color: #0000001a;
}

h2{
    text-align: center;
    color: white;
    font-weight: 64px;
}

div.pivo{
    width: 100vw;
    height: 300px;
    display: flex;
    justify-content: center;
    flex-direction: column;
}

div.general_beer{
    display: flex;
    padding: 30px 100px;
    justify-content: space-evenly;
    flex-wrap: wrap
}

div.beer_position{
    height: 552px;
    width: 360px;
    border: 1px solid gray;
    display: inline-block;
    margin: 10px;
}

div.beer_position {
    font-size: 20px;
}

div.inline{
    display: flex;
    justify-content: space-between;
}

div.beer_position > img{
    height: 360px;
    width: 360px;
}

div.pivo_last{
    width: 100vw;
    height: 140px;
    display: flex;
    justify-content: center;
}

div.pivo_last > div.first_button{
    padding: 30px;
    box-sizing: border-box;
    text-align: center;
    height: 80px;
    width: 80px;
    margin-right: 20px;
    display: inline-block;
    border: 1px solid #FFFFFF
}

div.pivo_last > div.second_button{
    padding: 30px;
    box-sizing: border-box;
    text-align: center;
    height: 80px;
    width: 80px;
    display: inline-block;
    border: 1px solid #292929;
    color: #292929;
}

footer{
    display: flex;
    justify-content: center;
    padding: 20px;
}

.text{
    font-size: 20px;
}

div.footer_icons{
    background-image: url(Frame\ 18.png);
    height: 24px;
    width: 100px;
    margin-left: 20px;
}

section{
    display: flex;
    justify-content: center;
    padding: 20px;
}

@media (max-width: 676px){
    nav {
        display: flex;
        padding: 0 10px;
        justify-content: space-between;
    }
    
    nav::before{
        content: "";
        display: block;
        width: 46px;
        height: 46px;
        margin: 14px;
        background-size: contain;
        background-image: url(Vector\ \(1\).png);
    }

    nav > .buttons{
        display: none;
    }

    div.pivo{
        width: 100vw;
        height: 120px;
        display: flex;
        justify-content: center;
        flex-direction: column;
    }

    div.beer_position > img{
        height: 152px;
        width: 152px;
    }

    div.beer_position{
        text-align: center;
        height: 300px;
        width: 191px;
        border: 1px solid gray;
        display: inline-block;
        margin: 10px;
    }

    div.general_beer{
        display: flex;
        padding: 30px 50px;
        justify-content: space-evenly;
        flex-wrap: wrap
    }

    div.beer_position > p{
        font-size: 14px;
        text-align: left;
        padding: 5px;
    }

    div.beer_position {
        font-size: 14px;
    }

    footer{
        display: flex;
        justify-content: center;
        padding: 20px;
    }
}
